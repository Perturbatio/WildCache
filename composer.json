	"require": {
		"illuminate/support": "5.8.*",
		"illuminate/cache": "5.8.*"
	},
	"scripts": {
		"test": "phpunit tests"
	},
	"branch-alias": {
		"dev-master": "1.1-dev"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Perturbatio\\WildCache\\WildCacheProvider"
			]
		}
	}
