{
	"name": "perturbatio/wildcache",
	"description": "",
	"license": "MIT",
	"keywords": [
		"laravel",
		"cache",
		"wildcard"
	],
	"authors": [
		{
			"name": "Kris Kelly",
			"email": "bertpotato@gmail.com"
		}
	],
	"require": {
		"illuminate/support": "^6|^7|^8|^9",
		"illuminate/cache": "^6|^7|^8|^9"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.3",
		"orchestra/testbench": "^6.0|^7.7"
	},
	"autoload": {
		"psr-4": {
			"Perturbatio\\WildCache\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Perturbatio\\WildCache\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit tests"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Perturbatio\\WildCache\\WildCacheProvider"
			]
		}
	}
}
