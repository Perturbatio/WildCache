{
	"name": "perturbatio/wildcache",
	"description": "",
	"license": "MIT",
	"keywords": [
		"laravel",
		"cache",
		"wildcard"
	],
	"authors": [
		{
			"name": "Kris Kelly",
			"email": "bertpotato@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Perturbatio\\WildCache\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "8.*",
		"orchestra/testbench": "3.8.*"
	},
